  "flow": [
    {
      "id": 1,
      "module": "webhook:CustomWebhook",
      "version": 1,
      "parameters": {
        "hook": "custom",
        "maxResults": 1
      },
      "mapper": {},
      "metadata": {
        "designer": {
          "x": 0,
          "y": 0
        }
      }
    },
    {
      "id": 2,
      "module": "http:ActionSendRequest",
      "version": 3,
      "parameters": {
        "url": "https://YOUR_APP_URL/webhook/monday",
